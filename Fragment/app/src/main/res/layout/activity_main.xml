<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.android.fragment.MainActivity">

    <fragment
        android:id="@+id/one_fragment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:name="com.example.android.fragment.OneFragment"
        tools:layout="@layout/fragment_one"/>

    <fragment
        android:id="@+id/two_fragment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:name="com.example.android.fragment.TwoFragment"
        tools:layout="@layout/fragment_two"/>


</LinearLayout>
